#include <stdio.h>

int main(void) {
int x,xx=0, y, m=3, n=3, test=0,teste=0,o=0, x1=0,w,z, y2, o2, x2, yy=0;

float matrizy[3][3]={0}, matriz1y[3][3]={2}, matrizx[3][3]={0}, matriz1x[3][3]={2},angulox[3][3]={1}, anguloy[3][3]={1}, soma=0;


  for(x=0;x<m;x++){
  for(y=0;y<n;y++){
    matrizy[x][y]=0;
    matrizx[x][y]=0;
    matriz1y[x][y]=2;
    matriz1x[x][y]=2;
    angulox[x][y]=1;
    anguloy[x][y]=1;
      
  }
}
  
matrizy[0][0]=0;
matrizy[0][1]=0;
matrizy[1][0]=0;
matrizy[2][1]=0;
matrizy[1][2]=0;
matrizy[2][2]=4;
  

matrizx[0][1]=0;
matrizx[1][0]=0;
matrizx[0][2]=0;
matrizx[2][0]=0;

matriz1y[0][0]=1;
matriz1y[0][1]=1;
matriz1y[1][0]=1;
matriz1y[2][1]=1;
matriz1y[1][2]=1;
matriz1y[2][2]=0;

matriz1x[0][1]=1;
matriz1x[1][0]=1;
matriz1x[0][2]=1;
matriz1x[2][0]=1;



angulox[1][0]=0.5;
angulox[0][1]=0.5;
anguloy[0][1]=0.5;
anguloy[1][0]=0.5;



while(matriz1x[0][2]==1){
 for(x=0;x<m;x++){
   for(y=0;y<n;y++){
     if(matriz1y[x][y]==0){
       yy++;
     }
     if(matriz1y[x][y]==1){
       
       xx++;
       
     }
   }
   
   if(xx==1 && yy>0){
     
     test=1;
   }
   else{
     test=0;
   }
   
   if(test==1){
   for(x1=0;x1<n;x1++){
     if(matriz1y[x][x1]==1){
       
       for(o=0;o<n;o++){
       soma=soma+matrizy[x][o];
      
       }
          
       soma=soma/anguloy[x][x1];
       
       matrizy[x][x1]=soma*-1;
       matrizy[x1][x]=soma;
       
       matriz1y[x][x1]=0;
       matriz1y[x1][x]=0;

       if(anguloy[x][x1]>0 && anguloy[x][x1]<1){
         matrizx[x][x1]=matrizy[x][x1]*angulox[x][x1];
         matrizx[x1][x]=matrizx[x][x1]*-1;
         matriz1x[x1][x]=0;
         matriz1x[x][x1]=0;
       }
       
       
     }
   }
     
   }
   xx=0;
   yy=0;
   test=0;
   soma=0;

   for(y2=0;y2<n;y2++){
     if(matriz1x[x][y2]==0){
       yy++;
     }
     
     if(matriz1x[x][y2]==1){
       
       xx++;
       
     }
   }
   
   if(xx==1 && yy>0){
     test=1;
   }
   else{
     test=0;
   }
   
   if(test==1){
   for(x2=0;x2<n;x2++){
     if(matriz1x[x][x2]==1){
       
       for(o2=0;o2<n;o2++){
       
       soma=soma+matrizx[x][o2];
      
       }
       
        
       soma=soma/angulox[x][x2];
      
     
       matrizx[x][x2]=soma*-1;
       matrizx[x2][x]=soma;
       matriz1x[x][x2]=0;
       matriz1x[x2][x]=0;

       
       
       if(angulox[x][x2]>0 && angulox[x][x2]<1){
         matrizy[x][x2]=matrizx[x][x2]*anguloy[x][x2];
         matrizy[x2][x]=matrizy[x][x2]*-1;
         matriz1y[x][x2]=0;
         matriz1y[x2][x]=0;
       }
       
     }
   }
     
   }
   xx=0;
   yy=0;
   test=0;
   soma=0;
   
   }
}
  
  printf("%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n%.2f\n%.2f",matrizy[0][0],
matrizy[0][1],
matrizy[0][2],
matrizy[1][0],
matrizy[1][1],
matrizy[1][2],
matrizy[2][0],
matrizy[2][1],
matrizx[0][2],
matrizx[2][0]
    
 );

   
  return 0;
}
